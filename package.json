{
  "name": "@jitsi/sdp-simulcast",
  "description": "A simple npm module that enables Chrome's native simulcast",
  "author": "",
  "version": "0.4.0",
  "stability": "unstable",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jitsi/sdp-simulcast.git"
  },
  "keywords": [
    "sdp",
    "webrtc",
    "simulcast"

    "karma-jasmine": "*",
    "karma-browserify": "5.1.0",
    "coveralls": "^2.11.1",
    "jscoverage": "^0.5.5",
    "nodeunit": "^0.9.0"
  },
  "bugs": {
    "url": "https://github.com/jitsi/sdp-simulcast/issues"
  },
  "homepage": "https://github.com/jitsi/sdp-simulcast#readme",
